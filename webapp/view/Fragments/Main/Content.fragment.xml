			<OverflowToolbar >
				<Title text="({= ${jsonModel>/TransferTablosu}.length})"></Title>
				<ToolbarSpacer ></ToolbarSpacer>
				<Button text="{i18n>SIL}" press="onSilButtonPress" icon="sap-icon://delete" type="Reject"/>
